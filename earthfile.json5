{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  regexManagers: [
    {
      customType: "regex",
      fileMatch: ["Earthfile"],
      matchStrings: [
        "FROM[\\s]+(--[\\w]+[\\s]+)*(?<depName>[a-zA-Z][a-zA-Z0-9_.\\-/]*):(?<currentValue>\\w[\\w.\\-]{0,127})",
      ],
      datasourceTemplate: "docker",
      versioningTemplate: "docker",
    },
  ],
}
